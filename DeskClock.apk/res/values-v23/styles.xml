    <style name="Base.Theme.AppCompat" parent="@style/Base.V23.Theme.AppCompat" />
    <style name="Base.Theme.AppCompat.Light" parent="@style/Base.V23.Theme.AppCompat.Light" />
    <style name="TimeLabelTextAppearance" parent="@style/TextAppearance.AppCompat">
        <item name="android:textSize">60.0dip</item>
        <item name="android:textColor">@color/timepicker_time</item>
